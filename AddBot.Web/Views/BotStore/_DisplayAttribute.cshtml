@model List<BotAttributeMaster>
@{
    if (Model != null && Model.Count > 0)
    {
        int serialNo = 1;
        <div>
            <table style="width: 100%; height: 100%" class="table table-striped table-bordered table-condensed">
                <thead class="success">
                    <tr>
                        <th>#</th>
                        <th>Attribute Name</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@serialNo</td>
                            <td> @item.AttributeName</td>
                        </tr>
                        serialNo = serialNo + 1;
                    }
                </tbody>
            </table>
        </div>
    }
}